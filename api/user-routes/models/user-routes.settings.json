{
  "kind": "collectionType",
  "collectionName": "user_routes",
  "info": {
    "name": "User routes",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "sites": {
      "type": "component",
      "repeatable": true,
      "component": "order.sites"
    },
    "public": {
      "type": "boolean",
      "default": false,
      "required": false
    },
    "distances": {
      "type": "json"
    },
    "owner": {
      "type": "string",
      "private": true,
      "required": false
    }
  }
}
