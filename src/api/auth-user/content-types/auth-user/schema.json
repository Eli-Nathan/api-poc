{
  "kind": "collectionType",
  "collectionName": "auth_users",
  "info": {
    "singularName": "auth-user",
    "pluralName": "auth-users",
    "displayName": "Auth users",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": false
    },
    "email": {
      "type": "email",
      "required": true,
      "private": true
    },
    "edit_requests": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::edit-request.edit-request"
    },
    "user_routes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-route.user-route"
    },
    "level": {
      "type": "enumeration",
      "enum": [
        "nomad",
        "wanderer",
        "explorer",
        "adventurer",
        "pioneer"
      ],
      "default": "nomad"
    },
    "score": {
      "type": "integer",
      "default": 0
    },
    "user_id": {
      "type": "string",
      "unique": true,
      "required": false
    },
    "avatar": {
      "type": "text"
    },
    "addition_requests": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::addition-request.addition-request"
    },
    "profile_pic": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "favourites": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::site.site"
    }
  }
}
