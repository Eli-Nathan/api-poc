{
  "kind": "collectionType",
  "collectionName": "edit_requests",
  "info": {
    "singularName": "edit-request",
    "pluralName": "edit-requests",
    "displayName": "Edit requests"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "site": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::site.site",
      "inversedBy": "edit_requests"
    },
    "data": {
      "type": "json"
    },
    "images": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "submitted",
        "pending",
        "moderating",
        "complete",
        "rejected"
      ]
    }
  }
}
