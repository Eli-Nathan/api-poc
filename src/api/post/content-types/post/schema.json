{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Posts",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "content": {
      "type": "richtext",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "SEO": {
      "displayName": "SEO block",
      "type": "component",
      "repeatable": true,
      "component": "seo.seo-block"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::post-tag.post-tag",
      "inversedBy": "posts"
    }
  }
}
